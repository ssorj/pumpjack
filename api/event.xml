<class name="event">
  The Proton event object.  It is passed to the methods on
  handler.

  <link node="handler"/>

  <group name="basic">
    <property type="container" value="[instance]" nullable="true">
      Get the container.
    </property>

    <property type="connection" value="[instance]" nullable="true">
      Get the connection.
    </property>

    <property type="session" value="[instance]" nullable="true">
      Get the session.
    </property>

    <property type="sender" value="[instance]" nullable="true">
      Get the sender.
    </property>

    <property type="receiver" value="[instance]" nullable="true">
      Get the receiver.
    </property>

    <property type="delivery" value="[instance]" nullable="true">
      Get the delivery.
    </property>

    <property type="delivery-ticket" value="[instance]" nullable="true">
      Get the delivery-ticket.
    </property>

    <property type="transport" value="[instance]" nullable="true">
      Get the transport.
    </property>
  </group>
</class>
