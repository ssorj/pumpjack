<module name="concurrent" experimental="true">
  <group name="concurrent" title="Concurrency interfaces">
    <class name="serialization" experimental="true">
      <group name="basic">
        <method name="inject">
          <input type="function"/>
          <output name="XXX" type="boolean"/>
        </method>
      </group>
    </class>

    <class name="serialized-handler" type="handler" experimental="true">
      <group name="basic">
        <method name="on-serialization-begin">
          <input type="serialization"/>
        </method>

        <method name="on-serialization-end">
          <input type="serialization"/>
        </method>
      </group>
    </class>

    <class name="driver" experimental="true">
      <group name="basic">
        <method name="listen">
          <input name="address" type="string"/>
          <input name="handler-factory" type="function"/>
        </method>

        <method name="run">
        </method>

        <method name="closed">
          <output type="boolean"/>
        </method>
      </group>
    </class>
  </group>
</module>
