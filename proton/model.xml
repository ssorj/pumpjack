          </property>

          <property name="LOCAL-CLOSED" value="4" type="integer">
            The local endpoint is closed.
          </property>

          <property name="REMOTE-UNINIT" value="8" type="integer">
            The remote endpoint is uninitialized.
          </property>

          <property name="REMOTE-ACTIVE" value="16" type="integer">
